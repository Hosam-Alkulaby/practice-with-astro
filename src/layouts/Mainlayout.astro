--- 
//component imports
import MainHead from "./MainHead.astro";

import Footer from "../components/Footer.astro";
import Nav from "../components/Nav.astro";




//style imports
import "../styles/global.css";
import PostHeader from "../components/PostHeader.astro";
import Category from "../pages/category/[category].astro";

const {title ="My Astro Blog",
description ="M musings about the Astro framework",
image,
frontmatter,
robots
} = Astro.props;
---

<html lang="en">
	<MainHead {title} {description} {image} 
	{frontmatter} {robots} />
	<body>
		<Nav />
		<main>
			<slot>Default Text</slot>
		</main>
		<Footer  />
	</body>
</html>